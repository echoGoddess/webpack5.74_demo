{
	// preset即一些plugins组成的合集
	"presets": [
	  [
		// 转译ES语法，比如箭头函数、const、let
		"@babel/preset-env",
		{
			// 按需引入ES API
		  "useBuiltIns": "usage",
		  "corejs": "3.26"
		}
	  ],
	[
		// 引用Typescript插件
		"@babel/preset-typescript",
		{
			// 支持所有文件扩展名，否则在vue文件中使用ts会报错
		  "allExtensions": true 
		}
	  ]
	]
  }