{
  "name": "webpackdemo",
  "sideEffects": [
    "*.scss"
  ],
  "version": "1.0.0",
  "description": "使用webpack 5.74构建前端项目",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js",
    "server": "nodemon ./build/server.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "generateAnalyzerFile": "webpack --profile --json > stats.json",
    "analyzer": "webpack-bundle-analyzer --port 8888 ./dist/stats.json"
  },
